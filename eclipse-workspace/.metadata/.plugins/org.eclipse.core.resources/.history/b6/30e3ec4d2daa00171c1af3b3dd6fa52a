import java.util.Random;

public class Employee {
	
	private int id = 0;
	private int age = 0;
	private int salary = 0;
	private String name = " ";
	private boolean employed = false;
	
	private Random r = new Random();
	
	public Employee() {
		age = r.nextInt(20) + 18;
		salary = r.nextInt(500) + 1000;
		name = "Jonas";
		
		System.out.println(age);
		System.out.println(salary);
		
		id = id + 1 ;
		
	}
	
}
